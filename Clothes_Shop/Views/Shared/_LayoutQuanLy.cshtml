<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/LayoutAdmin/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/LayoutAdmin/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/Content/LayoutAdmin/css/style.css" rel="stylesheet" />
    <link href="~/Content/LayoutAdmin/css/style-responsive.css" rel="stylesheet" />
    <link href="~/Content/LayoutAdmin/css/form.css" rel="stylesheet" />
    <script src="~/tinymce/js/tinymce/tinymce.min.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js" type="text/javascript"></script>
    <script>tinymce.init({ selector: 'textarea' });</script>
    <title>@ViewBag.Title</title>
</head>
<body>
    <!-- start: Header -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="@Url.Action("Index","TrangChu")"><span>TrangChu</span></a>

                <!-- start: Header Menu -->
                <div class="nav-no-collapse header-nav">
                    <ul class="nav pull-right">
                        
                        <!-- start: User Dropdown -->
                        <li class="dropdown">
                            @Html.Action("Account", "Admin")
                        </li>
                        <!-- end: User Dropdown -->
                    </ul>
                </div>
                <!-- end: Header Menu -->

            </div>
        </div>
    </div>
    <!-- start: Header -->

    <div class="container-fluid-full">
        <div class="row-fluid">

            <!-- start: Main Menu -->
            <div id="sidebar-left" class="span2">
                <div class="nav-collapse sidebar-nav">
                    <ul class="nav nav-tabs nav-stacked main-menu">
                        <li><a href="@Url.Action("QuanLyNhanVien","Admin")"><i class=""></i><span class="hidden-tablet"> Quản Lý Nhân Viên</span></a></li>
                        <li><a href="@Url.Action("QuanLyKhachHang","Admin")"><i class=""></i><span class="hidden-tablet"> Quản Lý Khách Hàng</span></a></li>

                        <li>
                            <a class="dropmenu" href="#"><i class=""></i><span class="hidden-tablet"> Thống kê hóa đơn</span></a>
                            <ul>
                                <li><a class="submenu" href="@Url.Action("DaThanhToan","Admin")"><i class=""></i><span class="hidden-tablet"> Đã thanh toán</span></a></li>
                                <li><a class="submenu" href="@Url.Action("ChuaThanhToan","Admin")"></i><span class="hidden-tablet"> Chưa thah toán</span></a></li>

                            </ul>
                        </li>
                        <li><a href="@Url.Action("ThongKeSanPham","Admin")"><i class=""></i><span class="hidden-tablet"> Thống kê sản phẩm</span></a></li>
                    </ul>
                </div>
            </div>
            <!-- end: Main Menu -->

            <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
                </div>
            </noscript>

            <!-- start: Content -->
            <div id="content" class="span10" style="overflow-x:auto">
                @RenderBody()
            </div><!--/.fluid-container-->
            <!-- end: Content -->
        </div><!--/#content.span10-->
    </div><!--/fluid-row-->

    <div class="modal hide fade" id="myModal">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">×</button>
            <h3>Settings</h3>
        </div>
        <div class="modal-body">
            <p>Here settings can be configured...</p>
        </div>
        <div class="modal-footer">
            <a href="#" class="btn" data-dismiss="modal">Close</a>
            <a href="#" class="btn btn-primary">Save changes</a>
        </div>
    </div>

    <div class="clearfix"></div>

    <footer>

        <p>
            

        </p>

    </footer>

    <!-- start: JavaScript-->
    <script src="~/Content/LayoutAdmin/js/jquery-1.9.1.min.js"></script>
    <script src="~/Content/LayoutAdmin/js/jquery-migrate-1.0.0.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery-ui-1.10.0.custom.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.ui.touch-punch.js"></script>

    <script src="~/Content/LayoutAdmin/js/modernizr.js"></script>

    <script src="~/Content/LayoutAdmin/js/bootstrap.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.cookie.js"></script>

    <script src='~/Content/LayoutAdmin/js/fullcalendar.min.js'></script>

    <script src='~/Content/LayoutAdmin/js/jquery.dataTables.min.js'></script>

    <script src="~/Content/LayoutAdmin/js/excanvas.js"></script>
    <script src="~/Content/LayoutAdmin/js/jquery.flot.js"></script>
    <script src="~/Content/LayoutAdmin/js/jquery.flot.pie.js"></script>
    <script src="~/Content/LayoutAdmin/js/jquery.flot.stack.js"></script>
    <script src="~/Content/LayoutAdmin/js/jquery.flot.resize.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.chosen.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.uniform.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.cleditor.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.noty.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.elfinder.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.raty.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.iphone.toggle.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.uploadify-3.1.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.gritter.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.imagesloaded.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.masonry.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.knob.modified.js"></script>

    <script src="~/Content/LayoutAdmin/js/jquery.sparkline.min.js"></script>

    <script src="~/Content/LayoutAdmin/js/counter.js"></script>

    <script src="~/Content/LayoutAdmin/js/retina.js"></script>

    <script src="~/Content/LayoutAdmin/js/custom.js"></script>
    <!-- end: JavaScript-->

</body>
</html>
